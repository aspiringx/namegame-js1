{
   "name": "namegame-js1",
   "version": "0.0.1",
   "description": "NameGame app js backend using koe.js, TypeORM and GraphQL",
   "main": "build/index.js",
   "type": "module",
   "scripts": {
      "test": "echo \"Error: no test specified\" && exit 1",
      "start": "npx tsx ./src/index.ts",
      "watch": "npx tsx watch ./src/index.ts",
      "typeorm": "npx typeorm-ts-node-es",
      "migration:run": "npx tsx ./node_modules/typeorm/cli.js migration:run -d ./src/data-source.ts",
      "migration:generate": "npx tsx ./node_modules/typeorm/cli.js migration:generate -d ./src/data-source.ts"
   },
   "author": "AspiringX",
   "license": "ISC",
   "dependencies": {
      "@koa/router": "^12.0.1",
      "dotenv": "^16.4.5",
      "koa": "^2.15.0",
      "koa-graphql": "^0.12.0",
      "mysql2": "^3.9.2",
      "reflect-metadata": "^0.1.13",
      "typeorm": "^0.3.20"
   },
   "devDependencies": {
      "@types/koa": "^2.15.0",
      "@types/koa__router": "^12.0.4",
      "@types/koa-graphql": "^0.8.11",
      "@types/node": "^18.11.9",
      "ts-node": "^10.9.2",
      "tsx": "^4.7.1",
      "typescript": "^5.3.3"
   }
}
